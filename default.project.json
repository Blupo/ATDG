        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",

            "Nevermore": {
                "$path": "Nevermore/loader/ReplicatedStorage/Nevermore"
            },

            "Shared": {
                "$path": "game/ReplicatedStorage/Shared",
                "$ignoreUnknownInstances": true,

                "t": {
                    "$path": "t"
                }
            },

            "UnitData": {
                "$path": "game/ReplicatedStorage/UnitData"
            }
        },

        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$path": "game/ServerScriptService",
            "$ignoreUnknownInstances": true,

